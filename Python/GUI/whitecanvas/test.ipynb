    "import numpy as np\n",
    "from whitecanvas import new_canvas\n",
    "\n",
    "canvas = new_canvas(\"matplotlib:qt\")\n",
    "\n",
    "x = np.linspace(-np.pi, np.pi, 100)\n",
    "\n",
    "line = canvas.add_line(np.cos(x*2), np.sin(x*3), color=\"r\")\n",
    "markers = canvas.add_markers(np.cos(x), np.sin(x), color=\"gray\")\n",
    "\n",
    "canvas.show()\n"
   ]
